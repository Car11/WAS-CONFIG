<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://www.springframework.org/schema/jee http://www.springframework.org/schema/jee/spring-jee-2.0.xsd" xmlns:jee="http://www.springframework.org/schema/jee">
    
    <!--========== Data Sources ==========-->
    <bean id="interrupciones2DS" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="java:comp/env/jdbc/interrupciones2DS" />
    </bean>

    <bean id="obrasDS" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="java:comp/env/jdbc/obrasDS" />
    </bean>

    <!--  <bean id="transformadores2DS" class="org.springframework.jndi.JndiObjectFactoryBean">
        <property name="jndiName" value="java:comp/env/jdbc/transformadores2DS" />
    </bean>-->
    
    <!-- LobHandler for JDBC drivers -->
    <bean id="defaultLobHandler" class="org.springframework.jdbc.support.lob.DefaultLobHandler" lazy-init="true"/>
    
    <!--========== Session Factory ==========-->
    <bean id="interrupcionesSF" class="org.springframework.orm.hibernate3.LocalSessionFactoryBean">
        <property name="dataSource" ref="interrupciones2DS" />
        <property name="lobHandler" ref="defaultLobHandler"/>
        <property name="mappingResources">
            <list>
				<value>cr/go/ice/interrupciones/domain/Animal.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Causa.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Circuito.hbm.xml</value>
                <value>cr/go/ice/interrupciones/domain/ComportamientoADPIR.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/ConsecutivoClor.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Correo.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Cuadrilla.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Dano.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Empleado.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/EquipoEspecial.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Fusible.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Glosario.hbm.xml</value>
                <value>cr/go/ice/interrupciones/domain/HistoricoTrasladoSeccion.hbm.xml</value>
                <value>cr/go/ice/interrupciones/domain/IndiceGlobalRegion.hbm.xml</value>
                <value>cr/go/ice/interrupciones/domain/IndiceCalidadRegion.hbm.xml</value>
                <value>cr/go/ice/interrupciones/domain/IndiceCalidadNacional.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Interrupcion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/InterrupcionGemela.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Material.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/NoPropiaSeccionamiento.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Oficina.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Parametro.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/PosteInstaladoRetirado.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Proteccion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Provolma.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Region.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Reporte.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Seccion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/SubEstacion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/SubEstacionSubRegion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/SubRegion.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/TipoVoltaje.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Trafo.hbm.xml</value>
 				<value>cr/go/ice/interrupciones/domain/Vehiculo.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/Pueblo.hbm.xml</value>
