				<value>cr/go/ice/interrupciones/domain/Auditoria.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/TiposCausa.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/TiposCausaEspecifica.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/DetalleCausa.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/UbicacionGeneralDano.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/UbicacionEspecificaDano.hbm.xml</value>
				<value>cr/go/ice/interrupciones/domain/UsuarioOficina.hbm.xml</value>
				
				<!-- Mapeos de SIGEdocWeb -->   
                <value>cr/go/ice/sige/carga/domain/DocArchivo.hbm.xml</value>    
                <value>cr/go/ice/sige/carga/domain/DocArchivoPuro.hbm.xml</value>          
                <value>cr/go/ice/sige/carga/domain/DocAplicacion.hbm.xml</value>
                <value>cr/go/ice/sige/carga/domain/DocRolArchivo.hbm.xml</value>
                <value>cr/go/ice/sige/carga/domain/DocRolUsuario.hbm.xml</value>
                <value>cr/go/ice/sige/carga/domain/DocTipoArchivo.hbm.xml</value>
                <value>cr/go/ice/sige/carga/domain/DocUsuarioCarga.hbm.xml</value>
                
                <!--======== UTILIDADESJAVA ========-->                
                <value>com/vvs/reporte/ReportesDinamicos.hbm.xml</value>
                <value>com/vvs/reporte/ReportesDinamicosAud.hbm.xml</value>
                <value>com/vvs/reporte/ReportesDinamicosSub.hbm.xml</value>
                <value>com/vvs/reporte/ReportesDinamicosMod.hbm.xml</value>  
				
				<!-- Mapeos de SIGEMailer -->	
				<value>cr/go/ice/sige/confmailer/domain/Mailer.hbm.xml</value> 	
            </list>
        </property>
        <property name="hibernateProperties">
            <props>
                <prop key="hibernate.dialect">org.hibernate.dialect.Oracle9Dialect</prop>
                <prop key="hibernate.show_sql">false</prop>
                <prop key="hibernate.use_outer_join">true</prop>
				<prop key="hibernate.format_sql">true</prop>
            </props>
        </property>
    </bean>
    
    <bean id="obrasSF" class="org.springframework.orm.hibernate3.LocalSessionFactoryBean">
        <property name="dataSource" ref="obrasDS" />
        <property name="mappingResources">
            <list>
				<value>cr/go/ice/obras/domain/Agencia.hbm.xml</value>
            </list>
        </property>
        <property name="hibernateProperties">
            <props>
                <prop key="hibernate.dialect">org.hibernate.dialect.Oracle9Dialect</prop>
                <prop key="hibernate.show_sql">false</prop>
                <prop key="hibernate.use_outer_join">true</prop>
                <prop key="hibernate.format_sql">true</prop>
            </props>
        </property>
    </bean>

