    <!--========== Data Access Objects ==========-->
    
    <!-- ===================Abstractos================= -->
    <bean id="abstractInterrupcionDAO" class="org.springframework.orm.hibernate3.support.HibernateDaoSupport" abstract="true">
        <property name="sessionFactory" ref="interrupcionesSF"/>
    </bean>

    <bean id="abstractObraDAO" class="org.springframework.orm.hibernate3.support.HibernateDaoSupport" abstract="true">
        <property name="sessionFactory" ref="obrasSF"/>
    </bean>
     
    <bean id="abstractTransformadorDAO" class="org.springframework.orm.hibernate3.support.HibernateDaoSupport" abstract="true">
        <property name="sessionFactory" ref="transformadoresSF"/>
    </bean>
    
    <!--<bean id="abstractConfmailerDAO" class="org.springframework.orm.hibernate3.support.HibernateDaoSupport" abstract="true">
        <property name="sessionFactory" ref="confmailerSF"/>
    </bean>-->    
    
