	<queryString>
		<![CDATA[SELECT
  m.aa,
  m.cod_oficina,
  m.numinterrup,
  m.lugar,
  m.codpueblo,
  m.codcalle,
  m.poste,
  m.secuencia,
  m.fecha_aviso,
  m.hora_aviso,
  v.tiempo_aviso,
  v.hrs_inicio,
  v.kms_inicio,
  floor((v.hrs_inicio - v.tiempo_aviso)*24) AS horaCuad,
  round((((v.hrs_inicio - v.tiempo_aviso)*24)-floor((v.hrs_inicio - v.tiempo_aviso)*24))*60,0) as minCuad,
  v.kms_llegada, (v.kms_llegada - v.kms_inicio) AS recorridoS,
  v.hrs_llegada,
  m.fecfin,
  m.horafin,
  ABS((EXTRACT(DAY  FROM (TO_DATE(to_char(m.fecfin, 'YYYYMMDD'),'YYYYMMDD') - TO_DATE(to_char(m.fecha_aviso, 'YYYYMMDD'),'YYYYMMDD')) DAY TO SECOND ) * 24) +
  (EXTRACT(HOUR FROM (TO_DATE(TO_CHAR(Floor(m.horafin))||':'||TO_CHAR((m.horafin-Floor(m.horafin))*100,'00'),'HH24: MI') -
  TO_DATE(TO_CHAR(Floor(m.hora_aviso))||':'||TO_CHAR((m.hora_aviso-Floor(m.hora_aviso))*100,'00'),'HH24: MI')) DAY TO SECOND))) horasRest,
  ABS((EXTRACT(DAY  FROM (TO_DATE(to_char(m.fecfin, 'YYYYMMDD'),'YYYYMMDD') - TO_DATE(to_char(m.fecha_aviso, 'YYYYMMDD'),'YYYYMMDD')) DAY TO SECOND ) * 24*60) +
  (EXTRACT(MINUTE FROM (TO_DATE(TO_CHAR(Floor(m.horafin))||':'||TO_CHAR((m.horafin-Floor(m.horafin))*100,'00'),'HH24: MI') -
  TO_DATE(TO_CHAR(Floor(m.hora_aviso))||':'||TO_CHAR((m.hora_aviso-Floor(m.hora_aviso))*100,'00'),'HH24: MI')) DAY TO SECOND))) minutosRest,
  v.hrs_restablece,
  v.kms_final,
  ABS((EXTRACT(DAY  FROM (TO_DATE(to_char(v.hrs_restablece, 'YYYYMMDD'),'YYYYMMDD') - TO_DATE(to_char(m.fecfin, 'YYYYMMDD'),'YYYYMMDD')) DAY TO SECOND ) * 24) +
  (EXTRACT(HOUR FROM (TO_DATE(TO_CHAR(v.hrs_restablece,'HH24: MI'),'HH24: MI') -
  TO_DATE(TO_CHAR(Floor(m.horafin))||':'||TO_CHAR((m.horafin-Floor(m.horafin))*100,'00'),'HH24: MI')) DAY TO SECOND))) horasReto,
  ABS((EXTRACT(DAY  FROM (TO_DATE(to_char(m.fecfin, 'YYYYMMDD'),'YYYYMMDD') - TO_DATE(to_char(m.fecfin, 'YYYYMMDD'),'YYYYMMDD')) DAY TO SECOND ) * 24*60) +
  (EXTRACT(MINUTE FROM (TO_DATE(TO_CHAR(v.hrs_restablece,'HH24: MI'),'HH24: MI') -
  TO_DATE(TO_CHAR(Floor(m.horafin))||':'||TO_CHAR((m.horafin-Floor(m.horafin))*100,'00'),'HH24: MI')) DAY TO SECOND))) minutosReto,
  (v.kms_final - v.kms_inicio) AS kmRecorrido,
  m.comentario,
  m.fecini,

  TO_CHAR(
  FLOOR(
  ((((TO_CHAR(v.hrs_llegada, 'mi')))+
  ((TO_CHAR(v.hrs_llegada, 'HH24'))*60))
  -
  (((TO_CHAR(v.hrs_inicio, 'mi')))+
  ((TO_CHAR(v.hrs_inicio, 'HH24'))*60)))
  /60)
  , '00')
  || ':' ||
  TO_CHAR(
  MOD(
  (((TO_CHAR(v.hrs_llegada, 'mi')))+
  ((TO_CHAR(v.hrs_llegada, 'HH24'))*60)
  -
  ((TO_CHAR(v.hrs_inicio, 'mi')))+
  ((TO_CHAR(v.hrs_inicio, 'HH24'))*60))

  , 60)
  , '00')

  AS TIEMPOSITIO

  FROM eve01m m, eve05m v
  WHERE v.num_vehiculo = $P{pVehiculo}
  AND to_date(to_char(m.fecini,'dd/MM/yyyy'),'dd/MM/yyyy') >=  to_date($P{pFechaInicio},'dd/MM/yyyy')
  AND to_date(to_char(m.fecfin,'dd/MM/yyyy'),'dd/MM/yyyy') <=  to_date($P{pFechaFin},'dd/MM/yyyy')
  AND m.aa = v.aa
  AND m.cod_oficina = v.cod_oficina
  AND m.numinterrup = v.numinterrup
  ORDER BY m.fecha_aviso, m.hora_aviso asc]]>
	</queryString>
