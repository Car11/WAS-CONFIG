	<queryString>
		<![CDATA[SELECT DISTINCT
---Numero Interrupcion
eve01m.numinterrup as interrupcion,eve01m.aa as ano, eve01m.cod_oficina as oficina,
---DESCSECCION-EVE06C
(select EVE06C.NOM_SECCION
from EVE06C--, eve01m
where EVE06C.cir=eve01m.cir and EVE06C.sub=eve01m.sub  and EVE06C.subregion=eve01m.subregion and eve01m.cir=EVE06C.cir
AND rownum = 1)descSeccion, EVE06C.SECCION,
---Comentario
eve01m.comentario,eve01m.FECINI,eve01m.FECfin,
---CAUSA
(SELECT eve09c.cod_causa  FROM eve09c WHERE eve09c.cod_causa     = eve01m.causa1  AND rownum = 1) codCausa,
(SELECT eve09c.nom_causa  FROM eve09c WHERE eve09c.cod_causa     = eve01m.causa1  AND rownum = 1) descCausa
FROM eve21m,eve01m,EVE06C
WHERE

eve01m.SECCION = NVL($P{pSeccion},eve01m.SECCION)
AND EVE06C.reg=NVL($P{pRegion},EVE06C.REG)
AND eve06c.SUBREGION = NVL($P{pSubregion},eve06c.SUBREGION)
AND eve06c.SUB = NVL($P{pSubestacion},eve06c.SUB)
AND eve06c.CIR = NVL($P{pCircuito},eve06c.CIR)
AND eve21m.indicador = NVL($P{pMovimiento},eve21m.indicador)
AND eve21m.tipoposte = NVL($P{pCalidad},eve21m.tipoposte)
AND eve01m.FECINI >= $P{pFechaInicio} AND eve01m.FECfin <= $P{pFechaFin}
AND eve01m.cod_material = 201
AND eve01m.REG=EVE06C.REG AND eve01m.SUBREGION =EVE06C.SUBREGION  AND eve01m.SUB=eve06c.SUB
AND eve01m.CIR =eve06c.CIR AND eve01m.SECCION = eve06c.SECCION

ORDER BY SECCION, eve01m.aa, eve01m.cod_oficina, eve01m.numinterrup]]>
	</queryString>
