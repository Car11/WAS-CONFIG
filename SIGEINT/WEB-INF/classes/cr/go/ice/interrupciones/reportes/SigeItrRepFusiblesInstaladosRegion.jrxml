	<queryString>
		<![CDATA[SELECT
COUNT(eve01m.REG),
eve04m.TIPO_INSTALA,
eve04m.CAPACIDAD_INSTALA,
eve05c.NOM_REGION,
eve01m.REG

FROM eve01m
INNER JOIN eve05c ON (eve01m.REG = eve05c.REG)
INNER JOIN eve04m ON (eve01m.AA = eve04m.AA) AND (eve01m.COD_OFICINA = eve04m.COD_OFICINA) AND (eve01m.NUMINTERRUP = eve04m.NUMINTERRUP)

WHERE (eve01m.reg IN ($P!{itemsSTR}) OR $P{cantidadItems} = 0)

GROUP BY eve01m.REG, eve04m.TIPO_INSTALA, eve04m.CAPACIDAD_INSTALA, eve05c.NOM_REGION
ORDER BY eve01m.REG, eve04m.TIPO_INSTALA, eve04m.CAPACIDAD_INSTALA]]>
	</queryString>
