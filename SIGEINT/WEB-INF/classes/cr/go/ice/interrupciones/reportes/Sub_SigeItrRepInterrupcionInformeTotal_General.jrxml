	<parameter name="pCalidad" isForPrompting="false" class="java.lang.Integer"/>
	<queryString><![CDATA[SELECT 
COUNT(case when (eve21m.tipoposte = 1 AND eve21m.indicador=1) then 1 else null end) "ConcretoInstalado",
COUNT(case when (eve21m.tipoposte = 1 AND eve21m.indicador=0) then 1 else null end) "ConcretoRetirado",
COUNT(case when (eve21m.tipoposte = 2 AND eve21m.indicador=1) then 1 else null end) "MaderaInstalado",
COUNT(case when (eve21m.tipoposte = 2 AND eve21m.indicador=0) then 1 else null end) "MaderaRetirado",
COUNT(case when (eve21m.tipoposte = 3 AND eve21m.indicador=1) then 1 else null end) "RielInstalado",
COUNT(case when (eve21m.tipoposte = 3 AND eve21m.indicador=0) then 1 else null end) "RielRetirado",
COUNT(case when (eve21m.tipoposte = 4 AND eve21m.indicador=1) then 1 else null end) "AutoSoportadoInstalado",
COUNT(case when (eve21m.tipoposte = 4 AND eve21m.indicador=0) then 1 else null end) "AutoSoportadoRetirado",
COUNT(case when (eve21m.tipoposte = 5 AND eve21m.indicador=1) then 1 else null end) "MetalInstalado",
COUNT(case when (eve21m.tipoposte = 5 AND eve21m.indicador=0) then 1 else null end) "MetalRetirado",
COUNT(DECODE(eve21m.indicador, 1, 1,NULL)) "Instalado",
COUNT(DECODE(eve21m.indicador, 0, 1,NULL)) "Retirado"
FROM eve01m,eve21m
WHERE	eve21m.numinterrup=$P{pInterrupcion} 
	AND eve21m.cod_oficina=$P{pOficina}  
	AND eve21m.ano=$P{pAno}
  	AND eve21m.indicador = NVL($P{pMovimiento},eve21m.indicador)
	AND eve21m.tipoposte = NVL($P{pCalidad},eve21m.tipoposte)
	AND eve21m.ano= eve01m.aa AND eve01m.cod_oficina = eve21m.cod_oficina AND eve21m.numinterrup = eve01m.numinterrup]]></queryString>

