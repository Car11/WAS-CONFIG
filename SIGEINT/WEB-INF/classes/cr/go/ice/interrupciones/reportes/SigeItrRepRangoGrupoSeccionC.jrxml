	<parameter name="tipoVoltaje" class="java.lang.String"/>
	<parameter name="causaCatastrofe1" class="java.lang.Integer"/>
	<parameter name="subestacion" class="java.lang.Integer"/>
	<parameter name="circuito" class="java.lang.Integer"/>
	<parameter name="itemsSTR" class="java.lang.String"/>
	<parameter name="cantidadItems" class="java.lang.Integer"/>
	<parameter name="seccionInicial" class="java.lang.Long"/>
	<parameter name="seccionFinal" class="java.lang.Long"/>
	<parameter name="causaCatastrofe2" class="java.lang.Integer"/>
	<parameter name="nombreSeccionInicial" class="java.lang.String" isForPrompting="false"/>
	<parameter name="nombreSeccionFinal" class="java.lang.String" isForPrompting="false"/>
	<queryString>
		<![CDATA[(SELECT
eve01m.SECCION as seccion, eve06c.NOM_SECCION, eve01m.CIR, eve01m.SUB,
eve06c.KMS_SECCION AS KMS_SECCION_EVE06C,
eve06c.ABON_SECCION AS ABON_SECCION_EVE06C,
COUNT(eve01m.SECCION) AS NUMERO_INTERRUPCIONES,
SUM(eve01m.TIEFUE) AS DURACION_EVE01M,
SUM(eve01m.ABO_AFECTA) AS CLIENTES_AFECTADOS_EVE01M,
SUM(eve01m.HRS_ABO) AS HORAS_ABONADO_EVE01M,

(SELECT
nvl(SUM(eve16m.TIEFUE) ,0)
FROM eve16m

WHERE (TO_DATE(eve16m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve16m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve16m.COD_OFICINA = $P{codigoOficina}  )
  AND (eve16m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve16m.SUB = $P{subestacion} )
  AND (eve16m.CIR = $P{subestacion} )
  AND (eve16m.SECCION = eve01m.SECCION)) as DURACION_EVE16M,

(SELECT
nvl(SUM(eve16m.ABO_AFECTA) ,0)
FROM eve16m

WHERE (TO_DATE(eve16m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve16m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve16m.COD_OFICINA = $P{codigoOficina}  )
  AND (eve16m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve16m.SUB = $P{subestacion} )
  AND (eve16m.CIR = $P{subestacion} )
  AND (eve16m.SECCION = eve01m.SECCION)) as CLIENTES_AFECTADOS_EVE16M,

(SELECT
nvl(SUM(eve16m.HRS_ABO) ,0)
FROM eve16m

WHERE (TO_DATE(eve16m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve16m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve16m.COD_OFICINA = $P{codigoOficina}  )
  AND (eve16m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve16m.SUB = $P{subestacion})
  AND (eve16m.CIR = $P{subestacion} )
  AND (eve16m.SECCION = eve01m.SECCION)) as HORAS_ABONADO_EVE16M,

TO_NUMBER(TO_DATE($P{fechaFinal}, 'DD-MM-YYYY') - TO_DATE($P{fechaInicio}, 'DD-MM-YYYY')) AS DIF_DIAS,
TO_NUMBER((TO_NUMBER(TO_DATE($P{fechaFinal}, 'DD-MM-YYYY') - TO_DATE($P{fechaInicio}, 'DD-MM-YYYY'))) * 24) AS HP

FROM eve01m
INNER JOIN eve06c ON (eve01m.SECCION = eve06c.SECCION) AND (eve01m.SUB = eve06c.SUB) AND (eve01m.CIR = eve06c.CIR)

WHERE (TO_DATE(eve01m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve01m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve01m.CAUSA1 <> $P{causaCatastrofe1} )
  AND (eve01m.CAUSA1 = $P{causaCatastrofe2} OR $P{causaCatastrofe2} = 0 )
  AND (eve01m.COD_OFICINA = $P{codigoOficina} )
  AND (eve01m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve01m.SUB = $P{subestacion} )
  AND (eve01m.CIR = $P{circuito} )
  AND (eve01m.SECCION IN ($P!{itemsSTR}) OR $P{cantidadItems} = 0 OR  $P{seccionFinal} > 0)
  AND ((eve01m.SECCION >= $P{seccionInicial} AND eve01m.SECCION <= $P{seccionFinal}) OR 0 = $P{seccionFinal})

GROUP BY eve01m.SUB, eve01m.CIR, eve01m.SECCION, eve06c.NOM_SECCION, eve06c.KMS_SECCION, eve06c.ABON_SECCION
--ORDER BY eve01m.SUB, eve01m.CIR, eve01m.SECCION

union all

SELECT

eve16m.SECCION as seccion, eve06c.NOM_SECCION, eve16m.CIR, eve16m.SUB,
eve06c.KMS_SECCION AS KMS_SECCION_EVE06C,
eve06c.ABON_SECCION AS ABON_SECCION_EVE06C,
COUNT(eve16m.SECCION) AS NUMERO_INTERRUPCIONES,
SUM(eve16m.TIEFUE) AS DURACION_EVE01M,
SUM(eve16m.ABO_AFECTA) AS CLIENTES_AFECTADOS_EVE01M,
SUM(eve16m.HRS_ABO) AS HORAS_ABONADO_EVE01M,
0 AS DURACION_EVE16M,
0 AS CLIENTES_AFECTADOS_EVE16M,
0 AS HORAS_ABONADO_EVE16M,
TO_NUMBER(TO_DATE($P{fechaFinal}, 'DD-MM-YYYY') - TO_DATE($P{fechaInicio}, 'DD-MM-YYYY')) AS DIF_DIAS,
TO_NUMBER((TO_NUMBER(TO_DATE($P{fechaFinal}, 'DD-MM-YYYY') - TO_DATE($P{fechaInicio}, 'DD-MM-YYYY'))) * 24) AS HP

FROM eve16m
INNER JOIN eve06c ON (eve16m.SECCION = eve06c.SECCION) AND (eve16m.SUB = eve06c.SUB) AND (eve16m.CIR = eve06c.CIR)
WHERE
(TO_DATE(eve16m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve16m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve16m.COD_OFICINA = $P{codigoOficina} )
  AND (eve16m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve16m.SUB = $P{subestacion} )
  AND (eve16m.CIR = $P{circuito})
  AND (eve16m.SECCION IN ($P!{itemsSTR}) OR $P{cantidadItems} = 0 OR  $P{seccionFinal} > 0)
  AND ((eve16m.SECCION >= $P{seccionInicial} AND eve16m.SECCION <= $P{seccionFinal}) OR 0 = $P{seccionFinal})
  AND (eve16m.seccion) NOT IN (SELECT DISTINCT eve01m.seccion FROM eve01m
WHERE
(TO_DATE(eve01m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') )
  AND (TO_DATE(eve01m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') )
  AND (eve01m.CAUSA1 <> $P{causaCatastrofe1} )
  AND (eve01m.CAUSA1 = $P{causaCatastrofe2} OR $P{causaCatastrofe2} = 0 )
  AND (eve01m.COD_OFICINA = $P{codigoOficina} )
  AND (eve01m.TIPO_VOLTAJE IN ($P!{tipoVoltaje}))
  AND (eve01m.SUB = $P{subestacion} )
  AND (eve01m.CIR = $P{circuito} )
  AND (eve01m.SECCION IN ($P!{itemsSTR}) OR $P{cantidadItems} = 0 OR  $P{seccionFinal} > 0)
  AND ((eve01m.SECCION >= $P{seccionInicial} AND eve01m.SECCION <= $P{seccionFinal}) OR 0 = $P{seccionFinal})
)
GROUP BY eve16m.SUB, eve16m.CIR, eve16m.SECCION, eve06c.NOM_SECCION, eve06c.KMS_SECCION, eve06c.ABON_SECCION
)
order by seccion]]>
	</queryString>
