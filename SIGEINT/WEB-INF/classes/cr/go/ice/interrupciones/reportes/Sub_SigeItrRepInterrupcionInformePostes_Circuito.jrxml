	<queryString>
		<![CDATA[SELECT DISTINCT
---Numero Interrupcion
eve01m.numinterrup AS INTERRUPCION,eve01m.aa AS ANO, eve01m.cod_oficina AS OFICINA,
---DESCCIRCUITO
(select eve01c.nom_circuito
from eve01c
where eve01c.cir=eve01m.cir and eve01c.sub=eve01m.sub and eve01m.cir=eve01c.cir
AND rownum = 1)descCircuito,eve01c.cir,
---Comentario
eve01m.comentario,eve01m.FECINI,eve01m.FECfin,
---CAUSA
(SELECT eve09c.cod_causa  FROM eve09c WHERE eve09c.cod_causa     = eve01m.causa1  AND rownum = 1) codCausa,
(SELECT eve09c.nom_causa  FROM eve09c WHERE eve09c.cod_causa     = eve01m.causa1  AND rownum = 1) descCausa
FROM eve21m,eve01m,eve01c
WHERE
eve01c.cir=NVL($P{pCircuito},eve01c.cir) AND eve01c.SUB=NVL($P{pSubestacion},eve01c.sub)
AND eve21m.indicador = NVL($P{pMovimiento},eve21m.indicador)
AND eve21m.tipoposte = NVL($P{pCalidad},eve21m.tipoposte)
AND eve01m.SECCION = NVL($P{pSeccion},eve01m.SECCION)
AND eve01m.REG=NVL($P{pRegion},eve01m.REG) AND eve01m.SUBREGION = NVL($P{pSubregion},eve01m.SUBREGION)
AND eve01m.FECINI >= $P{pFechaInicio} AND eve01m.FECfin <=$P{pFechaFin}
AND eve01m.cod_material = 201
AND eve01m.SUB=eve01c.SUB
AND eve01m.CIR =eve01c.cir

ORDER BY CIR, eve01m.aa, eve01m.cod_oficina, eve01m.numinterrup]]>
	</queryString>
