	<queryString>
		<![CDATA[SELECT
(select count(*) from eve01m where (cod_oficina = $P{codigoOficina} OR $P{codigoOficina} = 0) and sub = interrupcion.sub
 AND TO_DATE(fecini) >= to_date($P{fechaInicio},'dd/mm/yyyy')
 AND TO_DATE(fecfin) <= to_date($P{fechaFinal},'dd/mm/yyyy')
 AND(causa1 = $P{codigoCausa} OR $P{codigoCausa} = 0)
 AND(tipo_voltaje IN($P!{voltajesSTR}) OR $P{cantidadVoltajes}=0)
 AND causa1 <> $P{codigoCausaCatastrofe}
) as totalRegistros,
interrupcion.sub AS
codigoNivelRed,
(select nom_subestacion from eve03c where sub = interrupcion.sub) as nombreNivelRed,
  interrupcion.causa1 AS
codigoCausa,
(select nom_causa from eve09c where cod_causa = interrupcion.causa1) as nombreCausa,
  COUNT(*) AS
cantidadInterrupciones,
  SUM(interrupcion.tiefue) AS
tiempoFuera,
  SUM(interrupcion.abo_afecta) AS
abonadosafectados,
  SUM(interrupcion.hrs_abo) AS
horasabonado,
DECODE(SUM(interrupcion.tiefue)/DECODE((COUNT(*)),0,1,(COUNT(*))),0,SUM(interrupcion.tiefue),SUM(interrupcion.tiefue)/DECODE((COUNT(*)),0,1,(COUNT(*))))  as DPA,
DECODE(SUM(interrupcion.hrs_abo)/DECODE((select abon_subestacion from eve03c where sub = interrupcion.sub),0,1,(select abon_subestacion from eve03c where sub = interrupcion.sub)),SUM(interrupcion.hrs_abo),0,SUM(interrupcion.hrs_abo)/DECODE((select abon_subestacion from eve03c where sub = interrupcion.sub),0,1,(select abon_subestacion from eve03c where sub = interrupcion.sub))) AS DPIR,
DECODE(SUM(interrupcion.abo_afecta)/DECODE((select abon_subestacion from eve03c where sub = interrupcion.sub),0,1,(select abon_subestacion from eve03c where sub = interrupcion.sub)),SUM(interrupcion.abo_afecta),0,SUM(interrupcion.abo_afecta)/DECODE((select abon_subestacion from eve03c where sub = interrupcion.sub),0,1,(select abon_subestacion from eve03c where sub = interrupcion.sub))) AS FPI,
(select abon_subestacion from eve03c where sub = interrupcion.sub) AS clientesatendidosnivelred
FROM eve01m interrupcion
WHERE(interrupcion.cod_oficina = $P{codigoOficina} OR $P{codigoOficina} = 0)
 AND TO_DATE(interrupcion.fecini) >= to_date($P{fechaInicio},'dd/mm/yyyy')
 AND TO_DATE(interrupcion.fecfin) <= to_date($P{fechaFinal},'dd/mm/yyyy')
 AND(interrupcion.causa1 = $P{codigoCausa} OR $P{codigoCausa} = 0)
 AND(interrupcion.reg = $P{region})
 AND(interrupcion.subregion = $P{subregion})
 AND(interrupcion.sub IN ($P!{subestacionesSTR}) OR $P{cantidadSubestaciones}=0)
 AND(interrupcion.tipo_voltaje IN($P!{voltajesSTR}) OR $P{cantidadVoltajes}=0)
 AND interrupcion.causa1 <> $P{codigoCausaCatastrofe}
GROUP BY interrupcion.reg, interrupcion.subregion, interrupcion.sub,
  interrupcion.causa1
ORDER BY interrupcion.reg, interrupcion.subregion, interrupcion.sub,
  interrupcion.causa1]]>
	</queryString>
