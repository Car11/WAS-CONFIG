	<queryString><![CDATA[SELECT 
eve04m.TIPO_RETIRA, eve04m.TIPO_INSTALA, 
eve04m.CAPACIDAD_INSTALA, eve04m.CAPACIDAD_RETIRA, 

eve01m.REG, eve01m.SUBREGION, eve01m.SUB, eve01m.CIR, eve01m.SECCION, eve06C.NOM_SECCION, eve01C.NOM_CIRCUITO, 
eve01m.AA, eve01m.COD_OFICINA, eve01m.NUMINTERRUP, 
eve01m.LUGAR, 
eve01m.CODPUEBLO_EQUIPO, eve01m.CODCALLE, eve01m.POSTE, eve01m.SECUENCIA, 
eve01m.FASE_R, eve01m.FASE_S, eve01m.FASE_T, 
eve01m.COMENTARIO, 
eve01m.CAUSA1, eve09c.NOM_CAUSA

FROM eve01m 
INNER JOIN eve01c ON (eve01m.SUB = eve01c.SUB) AND (eve01m.CIR = eve01c.CIR) 
INNER JOIN eve06c ON (eve01m.SECCION = eve06c.SECCION) AND (eve01m.SUB = eve06c.SUB) AND (eve01m.CIR = eve06c.CIR) 
INNER JOIN eve04m ON (eve01m.AA = eve04m.AA) AND (eve01m.COD_OFICINA = eve04m.COD_OFICINA) AND (eve01m.NUMINTERRUP = eve04m.NUMINTERRUP) 
INNER JOIN eve09c ON (eve01m.CAUSA1 = eve09c.COD_CAUSA) 

WHERE (TO_DATE(eve01m.FECINI) >= TO_DATE($P{fechaInicio}, 'dd/mm/yyyy') ) 
  AND (TO_DATE(eve01m.FECFIN) <= TO_DATE($P{fechaFinal}, 'dd/mm/yyyy') ) 
  AND (eve01m.SUB = $P{subestacion} ) 
  AND (eve01m.SECCION IN ($P!{itemsSTR}) OR $P{cantidadItems} = 0)

ORDER BY eve01m.SECCION ASC]]></queryString>

