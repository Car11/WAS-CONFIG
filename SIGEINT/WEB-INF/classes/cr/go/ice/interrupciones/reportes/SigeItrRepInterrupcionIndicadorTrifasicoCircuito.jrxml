	<queryString><![CDATA[SELECT 
REG,
SUBREGION,
SUB,
CIR,
TIPO_VOLTAJE,
COD_VOLTAJE,
AA,
MES,
NUM_SALIDAS_ACUM,
NUM_SALIDAS,
FPI,
DPIC,
DPS,
DPIR,
ADPIC,
AFPI,
ADPS,
ADPIR,
DPA,
DPA_ACUM,
HRS_ABO,
ABO_AFECTA,
ABO_AFECTA_ACUM,
DURACION,
DURACION_ACUM,
HRS_ABO_ACUM,
INTERRUP_KM_ACUM,
INTERRUP_KM,
(select nom_circuito from eve01c where sub = indicador.sub and cir = indicador.cir) as nombreNivelRed,
(select KMS_circuito from eve01c where sub = indicador.sub and cir = indicador.cir) AS kilometrosnivelred,
(select ABON_circuito from eve01c where sub = indicador.sub and cir = indicador.cir) AS clientesatendidosnivelred
from eve13m indicador
where
indicador.tipo_voltaje = $P{tipoVoltaje}
and indicador.cod_voltaje = $P{codigoVoltaje}
and indicador.aa = $P{ano}
and indicador.mes = $P{mes}
$P!{restriccionSQL}
order by indicador.reg, indicador.subregion, indicador.sub, indicador.cir]]></queryString>

