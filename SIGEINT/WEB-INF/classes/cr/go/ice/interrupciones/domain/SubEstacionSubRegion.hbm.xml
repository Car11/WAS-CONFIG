<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.SubEstacionSubRegion" table="EVE01D">
        <composite-id name="subEstacionSubRegionID" class="cr.go.ice.interrupciones.domain.SubEstacionSubRegionID">
            <key-many-to-one name="oficina" column="COD_OFICINA" lazy="false"/>
            <key-many-to-one name="subRegion" lazy="false">
                <column name="SUBREGION"/>
                <column name="REG"/>
            </key-many-to-one>
            <key-property name="subEstacion" column="SUB" type="java.lang.Integer" length="3"/>
        </composite-id>
    </class>
</hibernate-mapping>