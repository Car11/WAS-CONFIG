<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.CambioSeccion" table="TMP_CAMBIO_SECCIONES">
        <composite-id name="id" class="cr.go.ice.interrupciones.domain.CambioSeccionID">
        	<key-property name="seccion" column="COD_SECCION" type="java.lang.Long" length="5"/>
            <key-property name="subEstacion" column="COD_SUBESTACION" type="java.lang.Integer" length="3"/>
            <key-property name="circuito" column="COD_CIRCUITO" type="java.lang.Integer" length="4"/>
            <key-property name="cedula" column="CEDULA" type="java.lang.String" length="9"/>
        </composite-id>
        <property name="region" column="COD_REGION" type="java.lang.Integer"  length="2" />
        <property name="subRegion" column="COD_SUBREGION" type="java.lang.Integer" length="2"/>
        <property name="codigoOficina" column="COD_OFICINA" type="java.lang.Integer" length="2"/>
        <property name="nombreSeccion" column="NOM_SECCION" type="java.lang.String" length="40"/>
        <property name="kmsSeccion" column="KM_SECCION" type="java.lang.Double" length="10" precision="2"/>
        <property name="abonadoSeccion" column="ABON_SECCION" type="java.lang.Long" length="6"/>
        <property name="congelado" column="CONGELADO" type="java.lang.Integer" length="1"/>
	</class>    
</hibernate-mapping>