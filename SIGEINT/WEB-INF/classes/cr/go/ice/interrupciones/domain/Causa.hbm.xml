        <property name="causaEspecifica" 	column="CAUSAESPEC" 	type="java.lang.Integer" 	length="1"/>
        <property name="detalleCausa" 		column="DETALLECAUSA" 	type="java.lang.Integer" 	length="2"/>
        <property name="estado" 			column="ESTADO" 		type="java.lang.Integer" 	length="1"/>
    </class>
</hibernate-mapping>