<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.InterrupcionGemela" table="EVE11D">
        <composite-id name="interrupcionGemelaID" class="cr.go.ice.interrupciones.domain.InterrupcionGemelaID">
            <key-many-to-one name="interrupcion" lazy="proxy">
				<column name="AA"/>
				<column name="COD_OFICINA"/>
				<column name="NUMINTERRUP"/>
			</key-many-to-one>
			<key-property name="numeroMovimiento" column="NUM_MOV" type="java.lang.Integer" length="3"/>
        </composite-id>
        <property name="fechaInicio" column="FECINI"  type="java.util.Date"/>
		<property name="fechaFin" column="FECFIN"  type="java.util.Date"/>
		<property name="horaInicio" column="HORAINI"  type="java.lang.Double" length="6" precision="2"/>
		<property name="horaFin" column="HORAFIN"  type="java.lang.Double" length="6" precision="2"/>
		<property name="tiefue" column="TIEFUE"  type="java.lang.Double" length="12" precision="2"/>
		<property name="horasAbonado" column="HRS_ABO"  type="java.lang.Double" length="10" precision="2"/>
		<property name="abonadoAfectado" column="ABO_AFECTA"  type="java.lang.Long" length="8"/>
    </class>
</hibernate-mapping>

