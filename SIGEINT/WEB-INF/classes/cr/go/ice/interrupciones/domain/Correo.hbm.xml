<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.Correo" table="EVE15C">
        <id name="correo" column="CORREO" type="java.lang.String" length="50">
            <generator class="assigned"/>
        </id>
        <property name="nombre" column="NOMBRE" type="java.lang.String"  length="30" />
        <property name="telefono" column="TELEFONO" type="java.lang.Long"  length="8" />
        <property name="codigoOficina" column="COD_OFICINA" type="java.lang.Integer"  length="2" />
        <property name="indicadorEnviado" column="IND_ENVIO" type="java.lang.Integer"  length="1" />        
    </class>
</hibernate-mapping>