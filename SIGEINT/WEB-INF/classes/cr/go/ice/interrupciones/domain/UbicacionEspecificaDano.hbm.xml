<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.UbicacionEspecificaDano" table="EVE_UBICAESPECDANO">
        <composite-id name="ubicacionEspecificaDanoID" class="cr.go.ice.interrupciones.domain.UbicacionEspecificaDanoID">
            <key-property name="danoEspecifico" column="DANOESPEC" type="java.lang.Integer" length="2"/>
            <key-many-to-one name="ubicacionGeneralDano" column="DANOGENERAL" lazy="false"/>
        </composite-id>
        <property name="descripcion" column="DESCRIPCION"  type="java.lang.String" length="250"/>
        <property name="estado" column="ESTADO" type="java.lang.Integer" length="1"/>
    </class>
</hibernate-mapping>