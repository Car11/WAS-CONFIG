<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.UsuarioOficina" table="EVE_OFICINACEDULA">
        <composite-id name="id" class="cr.go.ice.interrupciones.domain.UsuarioOficinaId">
            <key-many-to-one name="empleado" class="cr.go.ice.interrupciones.domain.Empleado" lazy="false">
                <column name="CEDULA" />
            </key-many-to-one>
            <key-many-to-one name="oficina" class="cr.go.ice.interrupciones.domain.Oficina" lazy="false">
                <column name="COD_OFICINA" />
            </key-many-to-one>
        </composite-id>
    </class>
</hibernate-mapping>