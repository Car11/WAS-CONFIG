<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="cr.go.ice.interrupciones.domain.TiposCausaEspecifica" table="EVE_TIPOCAUSAESPEC">
        <composite-id name="tiposCausaEspecificaID" class="cr.go.ice.interrupciones.domain.TiposCausaEspecificaID">
            <key-property name="causaEspecifica" column="CAUSAESPEC" type="java.lang.Integer" length="1"/>
            <key-many-to-one name="tiposCausa" column="TIPOCAUSA" lazy="false"/>
        </composite-id>
        <property name="descripcion" column="DESCRIPCION"  type="java.lang.String" length="250"/>
        <property name="estado" column="ESTADO" type="java.lang.Integer" length="1"/>
    </class>
</hibernate-mapping>