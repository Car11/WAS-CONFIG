		<property name="kilometrosFinal" column="KMS_FINAL"  type="java.lang.Double" length="12" precision="2" not-null="true"/>
		<property name="kilometrosLlegada" column="KMS_LLEGADA"  type="java.lang.Double" length="12" precision="2" not-null="true"/>
		<property name="fechaLocalizacion" column="FECHR_LOCALIZACION"  type="java.util.Date" not-null="false"/>
		<property name="kilometrosLocalizacion" column="KMS_LOCALIZACION"  type="java.lang.Double" length="12" precision="2" not-null="false"/>
    </class>
</hibernate-mapping>
